<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:src="clr-namespace:RzWpfSample_Shell"
                    xmlns:RzWpf="clr-namespace:RzWpf;assembly=RzWpf">

    <DataTemplate DataType="{x:Type src:LoginPageViewModel}">
        <src:LoginPageView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type src:ContentPageViewModel}">
        <src:ContentPageView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type RzWpf:MessageDialogViewModel}">
        <src:MessageDialogView/>
    </DataTemplate>

</ResourceDictionary>